{
  "name": "strawberry",
  "version": "1.1.2",
  "description": "LLM client(linux, mac, win)",
  "main": "./out/main/index.mjs",
  "author": "junki.cn",
  "homepage": "https://junki.cn",
  "repository": {
    "type": "git",
    "url": "https://github.com/classfang/strawberry.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "npm run prebuild && electron-vite",
    "prebuild": "node scripts/generate-build-info.js",
    "build": "npm run prebuild && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@langchain/community": "^0.3.19",
    "@langchain/core": "^0.3.24",
    "@types/markdown-it": "^14.1.2",
    "ajv": "^8.17.1",
    "electron-store": "^10.0.0",
    "langchain": "^0.3.7",
    "mammoth": "^1.8.0",
    "officeparser": "^5.1.1",
    "pdf-parse": "^1.1.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@element-plus/icons-vue": "^2.3.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/katex": "^0.16.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vscode/markdown-it-katex": "^1.1.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.9",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.13",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "element-plus": "^2.9.1",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "highlight.js": "^11.11.0",
    "html-to-image": "^1.11.11",
    "katex": "^0.16.15",
    "markdown-it": "^14.1.0",
    "openai": "^4.76.3",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "sass": "^1.83.0",
    "svelte": "^5.14.4",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-tsc": "^2.1.10"
  }
}
